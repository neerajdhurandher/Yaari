<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E96FF442"
    tools:context=".LoginActivity"
    android:id="@+id/loginpageid">

    <com.google.android.gms.common.SignInButton
        android:id="@+id/google_button"
        android:layout_width="@dimen/two_hundred_Dimen"
        android:layout_height="@dimen/seventy_Dimen"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="@dimen/ninety_Dimen"
        android:layout_marginTop="@dimen/one_hundred_eighty_Dimen"
        android:textSize="@dimen/thirty_SP_Dimen" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="@dimen/one_hundred_eighty_Dimen"
        android:layout_height="@dimen/sixty_Dimen"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/one_hundred_ten_Dimen"
        android:layout_marginTop="@dimen/three_hundred_thirty_Dimen"
        android:background="@color/cardview_white"
        android:paddingStart="@dimen/fifteen_Dimen"
        android:paddingEnd="@dimen/zero_Dimen"
        android:text="@string/T_C"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@+id/google_button" />

</RelativeLayout>